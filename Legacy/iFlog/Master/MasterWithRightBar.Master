<%@ Master Language="VB" AutoEventWireup="false" CodeBehind="MasterWithRightBar.master.vb"
    Inherits="BetterclassifiedsWeb.Master.MasterWithRightBar" MasterPageFile="~/Master/Default.Master" %>

<%@ Register Src="~/Controls/AdSpace.ascx" TagName="AdSpaces" TagPrefix="ucx" %>

<asp:Content ContentPlaceHolderID="head" runat="server" ID="cntHead">
    <asp:ContentPlaceHolder ID="HeadPlaceHolder" runat="server" />
</asp:Content>

<asp:Content ContentPlaceHolderID="ContentPlaceHolder1" runat="server" ID="cntMain">

    <div class="clearFloat">
        <div id="contentBody">
            <asp:ContentPlaceHolder ID="BodyPlaceHolder" runat="server" />
            <div id="rightSidebar">
                <div style="clear: both"></div>
                <ucx:AdSpaces ID="ucxAds" runat="server" Location="RightColumn" />
            </div>
        </div>
    </div>

</asp:Content>

