@model RemoveGuestCompleteViewModel

@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/_ApplicationLayout.cshtml";
}

<div class="container top-buffer-60">
    <h1 class="page-header">
        Guest Removed
    </h1>
    <div class="row">
        <div class="col-xs-12">
            <div class="alert alert-info-alt">
                The guest has been removed successfully.
            </div>
        </div>
       
    </div>
    <div class="row">
        <div class="col-sm-6">
            <a class="btn btn-default btn-block js-load" href="@Url.ManageGuests(Model.AdId, Model.EventId)">
                <i class="fa fa-arrow-circle-left"></i>
                Manage Guests
            </a>
        </div>

        <div class="col-sm-6">
            <a class="btn btn-default btn-block js-load" href="@Url.EventDashboard(Model.AdId)">
                <i class="fa fa-arrow-circle-left"></i>
                <span class="hidden-xs">Event</span> Dashboard
            </a>
        </div>
    </div>
</div>
