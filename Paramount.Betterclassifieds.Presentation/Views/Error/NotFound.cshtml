@using Paramount
@model dynamic

@{
    ViewBag.Title = "Classifieds - Not Found";
    Layout = Url.ClientUrl("~/Views/Shared/_ApplicationLayoutWithSearch.cshtml");
}


<div class="row">
    <div class="col-xs-6">
        <h1>Oooop. The page you are looking for doesn't exist!</h1>
    </div>
    <div class="col-sm-6">
        <div class="well-lg ">
            <p class="lead">
                What does this mean? What you are experiencing is a case of a 404 Error. A very common problem on the internet
                that appears when something you have clicked or searched for seems to have moved or never existed...
            </p>
        </div>
    </div>
</div>
