<script>
    var $paramount = (function (me) {
        
        me.url = {
            getRate: '@Url.GetRate()',
            uploadCropImage: '@Url.ImageUpload()',
            imgThumb : '@Url.Image("-1")',
            cancelCropImage: '@Url.Action("CancelCrop", "Image")',
            removeImage: '@Url.Action("RemoveImage", "Image")',
            cropImage: '@Url.Action("CropImage", "Image")'
        }

        return me;

    })($paramount || {});
</script>

