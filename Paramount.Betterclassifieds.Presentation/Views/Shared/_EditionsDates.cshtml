
<div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="editionsModal" aria-hidden="true" id="editionsModal">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel">Editions Dates</h4>
            </div>
            <div class="modal-body">
                <div class="row" data-bind="foreach: {data:publicationEditions, as :'publication'}">
                    <div class="col-xs-12 col-md-6">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th class="success">
                                        <span data-bind="text: publicationName"></span>
                                    </th>
                                </tr>
                            </thead>
                            <tbody data-bind="foreach: {data: editions, as : 'editionDate'}">
                                <tr>
                                    <td data-bind="text: editionDate"></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
                
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Ok</button>
            </div>
        </div>

    </div>
</div>