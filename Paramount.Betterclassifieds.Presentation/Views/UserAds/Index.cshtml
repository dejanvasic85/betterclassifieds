@model dynamic

@{
    Layout = "~/Views/Shared/_ApplicationLayout.cshtml";
    ViewBag.Title = "Manage my Ads";
}

@section Scripts
{
    <script>
    $paramount.ui.userAdsUI.init();
    </script>
}

<div class="container top-buffer-60">
    
    <h2 class="page-header">
        Event Management
    </h2>
    <p>
        You have been granted the privilage to manage the following events.
    </p>
    
    <div id="eventList">
        <event-list params="maxItems: 100, user: true"></event-list>
    </div>
</div>