<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="ConfigurationContext" value="Test" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
    <add key="BaseUrl" value="http://test-iflog.paramountit.com.au" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
  </appSettings>
  <connectionStrings>
    
    <add name="BetterclassifiedsConnection"
         connectionString="Data Source=localhost; Initial Catalog=iFlog;Persist Security Info=True;User ID=AppUser; Password=Paramount01"
         xdt:Transform="SetAttributes"
         xdt:Locator="Match(name)"/>
    
  </connectionStrings>
  <paramount xdt:Transform="Replace">
    <services>
      <milieu context="Test">
        <add key="Betterclassifieds" value="Data Source=localhost;Initial Catalog=iFlog;Persist Security Info=True; User ID=AppUser; Password=Paramount01" />
        <add key="AppUserConnection" value="Data Source=localhost;Initial Catalog=iFlogAppUser;Persist Security Info=True;User ID=AppUser;Password=Paramount01" />
      </milieu>
    </services>
    <broadcast>
      <add key="ConnectionString" value="Data Source=localhost;Initial Catalog=Broadcast;Persist Security Info=True; User ID=AppUser; Password=Paramount01" />
    </broadcast>
  </paramount>

  <system.net>
    <mailSettings xdt:Transform="Replace">
      <smtp deliveryMethod="SpecifiedPickupDirectory">
        <specifiedPickupDirectory pickupDirectoryLocation="C:\Paramount\MailDrop\"/>
      </smtp>
    </mailSettings>
  </system.net>
  
</configuration>